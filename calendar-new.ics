---
layout: none
permalink: /calendar-new.ics
---
BEGIN:VCALENDAR
VERSION:2.0
NAME:CSS Bristol Events
LAST-MODIFIED:{{ site.date | date: "%Y%m%dT%H%M00Z" }}
URL:https://www.cssbristol.co.uk/events
REFRESH-INTERVAL;VALUE=DURATION:6H
PRODID:-//CSS Bristol//NONSGML website//EN
{% for event in site.events %}BEGIN:VEVENT
UID:{{ event.path | replace: "_" "" | replace: "/", "-" | replace: ".md", ""}}@cssbristol.co.uk
DTSTAMP:{{ event.date |  date: "%Y%m%dT%H%M00Z" }}
DTSTART:{{ event.date | date: "%Y%m%dT%H%M00Z" }}
DTEND:{{ event.date_end | date: "%Y%m%dT%H%M00Z" }}
URL:https://www.cssbristol.co.uk/{{ event.url }}
IMAGE;VALUE=URI:https://www.cssbristol.co.uk/assets/images/contrib/events/{{ event.banner }}
ORGANIZER;CN="CSS":MAILTO:webmaster@cssbristol.co.uk
SUMMARY:{{ event.title }}
LOCATION:{{ event.location }}
END:VEVENT
{% endfor %}END:VCALENDAR
